datasource DS {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

model Post {
  id        Int      @default(autoincrement()) @id
  title     String
  body      String
  url       String
  createdAt DateTime @default(now())
}
